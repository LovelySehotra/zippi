{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/zippi/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/zippi/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/zippi",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "billpay": {
      "type": "application",
      "root": "apps/billpay",
      "entryFile": "main",
      "sourceRoot": "apps/billpay/src",
      "compilerOptions": {
        "tsConfigPath": "apps/billpay/tsconfig.app.json"
      }
    },
    "notification": {
      "type": "application",
      "root": "apps/notification",
      "entryFile": "main",
      "sourceRoot": "apps/notification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification/tsconfig.app.json"
      }
    },
    "payment": {
      "type": "application",
      "root": "apps/payment",
      "entryFile": "main",
      "sourceRoot": "apps/payment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment/tsconfig.app.json"
      }
    },
    "subscription": {
      "type": "application",
      "root": "apps/subscription",
      "entryFile": "main",
      "sourceRoot": "apps/subscription/src",
      "compilerOptions": {
        "tsConfigPath": "apps/subscription/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "zippi": {
      "type": "application",
      "root": "apps/zippi",
      "entryFile": "main",
      "sourceRoot": "apps/zippi/src",
      "compilerOptions": {
        "tsConfigPath": "apps/zippi/tsconfig.app.json"
      }
    }
  }
}